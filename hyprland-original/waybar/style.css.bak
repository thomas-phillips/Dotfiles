* {
    border: none;
    border-radius: 0px;
    /*font-family: VictorMono, Iosevka Nerd Font, Noto Sans CJK;*/
    font-family: Hack, Iosevka, FontAwesome, Noto Sans CJK;
    font-size: 15px;
    font-style: normal;
    min-height: 0;
}

window#waybar {
    background: transparent;
    color: #7e9aa6;
}

#custom-launcher {
    background: #001119;
    padding-left: 15px;
    padding-right: 14px;
    border-radius: 16px 0px 0px 16px;
    margin: 5px 0px 0px 5px;
}

#workspaces {
    background: #001119;
    margin: 5px 0px 0px 0px;
    padding: 0px 10px 0px 5px;
    border-radius: 0px 16px 16px 0px;
    /* border: solid 0px #7e9aa6; */
    font-weight: normal;
    font-style: normal;
}


#workspaces button {
    padding: 4px 4px;
    border-radius: 16px;
    /* color: #7e9aa6; */
    color: white;
}

#workspaces button.active {
    color: #ffffff;
    /* background-color: transparent; */
    border-radius: 16px;
}

#workspaces button:hover {
    background-color: #ffffff;
    color: black;
    border-radius: 20px;
    /* padding: 0px 5px 0px 5px; */
}

#custom-playerctl {
    background: #001119;
    margin: 5px 5px 0px 5px;
    padding: 0px 15px 0px 15px;
    border-radius: 16px;
    border: solid 0px #7e9aa6;
    font-weight: normal;
    font-style: normal;
}

#clock {
    color: #7e9aa6;
    background-color: #001119;
    margin: 5px 5px 0px 5px;
    padding: 0px 5px 0px 5px;
    border-radius: 16px;
    font-weight: bold;
}

#pulseaudio {
    color: #7e9aa6;
    margin: 5px 0px 0px 5px;
    padding: 0px 5px 0px 15px;
    background: #001119;
    border-radius: 16px 0px 0px 16px;
    font-weight: normal;
    font-style: normal;
}

#pulseaudio.muted {
    background: transparent;
    color: #7e9aa6;
    border-radius: 8px;
    margin-left: 0px;
}

#battery {
    color: #7e9aa6;
    margin: 5px 0px 0px 0px;
    padding: 0px 5px 0px 5px;
    background: #001119;
    font-weight: normal;
    font-style: normal;
}

#memory {
    color: #7e9aa6;
    margin: 5px 0px 0px 0px;
    padding: 0px 5px 0px 5px;
    background: #001119;
    font-weight: normal;
    font-style: normal;
}

#cpu {
    color: #7e9aa6;
    margin: 5px 0px 0px 0px;
    padding: 0px 5px 0px 5px;
    background: #001119;
    font-weight: normal;
    font-style: normal;
}

#network {
    color: #7e9aa6;
    background: #001119;
    margin: 5px 0px 0px 0px;
    padding: 0px 5px 0px 5px;
}


#tray {
    background: #001119;
    margin: 5px 0px 0px 0px;
    padding: 0px 5px 0px 5px;
}

#custom-powermenu {
    color: #7e9aa6;
    background: #001119;
    padding-left: 10px;
    padding-right: 14px;
    border-radius: 0px 16px 16px 0px;
    margin: 5px 5px 0px 0px;
}
